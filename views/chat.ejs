<!DOCTYPE html>
<html>

<head>
    <title>Selups</title>
    <link rel="stylesheet" href="/style/apprentice.css">
    <link rel="stylesheet" href="/style/style.css">
</head>

<body>
    <table id="messages">
        <tbody id="mtb">
            <% for(const message of messages) { %>
            <tr>
                <td class="info"><%=  message.id %></td>
                <td class="info"><%= new Date(message.date).toLocaleString("tr") %></td>
                <td class="message"><%=  message.msg %></td>
            </tr>
            <% } %>
        </tbody>
    </table>
    <form id="form" action="" data-username="<%= username %>">
        <input id="input" autocomplete="off" /><button>Send</button>
    </form>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/script/script.js"></script>
</body>

</html>